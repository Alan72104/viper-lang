struct Test {
    field: i32;
    another: i32;

    func @method(balls: i32) -> i32;
}

func Test@method(balls: i32) -> i32 {
    return this->another + balls;
}

func @main() -> i32 {
    let test: struct Test = struct Test { 1, 2 };
    let ptr: struct Test* = &test;

    return ptr->method(5);
}
