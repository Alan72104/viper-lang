func @printInt(num: i32) -> i32;
func @print(message: i8*) -> i32;

struct abc {
    test: i32;
    foo: i32;
    bar: i8*;
}

func @max(a: i32, b: i32) -> i32 {
    if (a > b) return a;
    return b;
}

func @main(argc: i32) -> i32 {
    let maximum: i32 = max(argc, 5);
    printInt(maximum);

    let test: struct abc;
    test.foo = 1;
    test.bar = "Hello World!\n";
    print(test.bar + 1);

    let something: struct abc* = &test;
    
    return something->foo;
}